{
  "name": "labpipe-client",
  "version": "0.0.0",
  "description": "LabPipe Client.",
  "main": "index.js",
  "scripts": {
    "pack": "electron-builder --dir",
    "dist": "electron-builder -mw --publish=always",
    "electron": "electron ."
  },
  "author": "Bo Zhao",
  "license": "NPOSL-3.0",
  "devDependencies": {
    "electron": "^6.0.10",
    "electron-builder": "^21.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/colin-bz/labpipe-client-public.git"
  },
  "build": {
    "appId": "org.labpipe.client",
    "files": ["**/*", "build/icon.*"],
    "mac": {
      "category": "productivity",
      "target": [
        {
          "target": "zip"
        }
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    }
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "electron-debug": "^3.0.1",
    "electron-log": "^3.0.8",
    "electron-settings": "^3.2.0",
    "nedb": "^1.8.0",
    "nedb-promises": "^4.0.0",
    "uuid": "^3.3.3"
  }
}
